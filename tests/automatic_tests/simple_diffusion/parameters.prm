set dim = 2
set global refinement = 7
set degree = 2
subsection Rectangular mesh
  set x size = 50.0
  set y size = 50.0
  #set z size = 50.0
  set x subdivisions = 1
  set y subdivisions = 1
  #set z subdivisions = 1
end
set time step = 5.0e-2
set number steps = 100
subsection output
  set condition = EQUAL_SPACING
  set number = 100
end
set boundary condition for C = Natural
set boundary condition for p = Natural
set Model constant diff = 0.01, double